#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>


&mt {
  //  flavor = "tap-preferred";
  // tapping_term_ms = <200>;
};

#define L1 0
#define L2 1
#define L3 2
#define L4 3

keymap {
    compatible = "zmk,keymap";
                
    l1_layer {
		bindings = <
		&kp Q &kp W &kp F &kp P &kp B 					  &kp J &kp L &kp U &kp Y &kp SEMICOLON 
		&mt LCTRL A &mt LALT R &mt LCMD S &mt LSHIFT T &kp G 			      &kp M &mt LSHIFT N &mt LCMD E &mt LALT I &mt LCTRL O
		&kp Z &kp X &kp C &kp D &kp V				      &kp K &kp H &kp COMMA &kp DOT &kp SLASH
          				    &lt L2 ESC &lt L3 RETURN      &lt L4 SPC &kp BACKSPACE
		>;
		};

    l2_layer {
		bindings = <
		&none &none &none &none &none        &kp DOWN &kp RIGHT &none &none &none
    &none &none &none &none &none        &none &none &none &none &none
    &none &none &none &none &none        &kp UP &kp LEFT &none &none &none
					            &none &none        &kp TAB &kp DELETE
		>;
		};
		
		l3_layer {
		bindings = <
		&none &none &none &none &none        &none &none &none &none &none
    &none &none &none &none &none        &none &none &none &none &none
    &none &none &none &none &none        &none &none &none &none &none
					            &none &none        &none &none
		>;
		};	

		l4_layer {
		bindings = <
		&none &none &none &none &none        &none &kp 7 &kp 8 &kp 9 &none
    &none &none &none &none &none        &none &kp 4 &kp 5 &kp 6 &none
    &none &none &none &none &none        &none &kp 1 &kp 2 &kp 3 &none
					            &none &none        &none &kp 0
		>;
    };	
	};
};
